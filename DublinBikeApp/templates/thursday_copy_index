<script>

	var marker = null;
	var map;
	function initMap() {
		var DublinPosition = {lat: 53.3484906, lng: -6.2551201};
		var DublinPosition2 = {lat: 53.3484906, lng: -7.2551201};
		map = new google.maps.Map(document.getElementById('map'), { zoom:10,
	  		center: DublinPosition
			});
		var marker = new google.maps.Marker({
	  		position: DublinPosition,
	  		map: map
			});
		var marker = new google.maps.Marker({
          		position: DublinPosition2,
          		map: map
 			}); 
		var marker = null;
	}


	var marker = null;
        function placeMarker(location, title){

                if (marker != null) {
                        marker.setMap(null);
                        }
                marker = new google.maps.Marker({
                        position: location,
                        animation: google.maps.Animation.BOUNCE,
                        title: title,
                        draggable:true
                        });
                
                marker.setMap(map);
                }

	var lagLang = [];
	var asd = { lat: -25.403, lng: 131.044 };
	lagLang.push(asd)

	var asd = { lat: -25.403, lng: 135.044 };
	lagLang.push(asd)

	var asd = { lat: -25.403, lng: 139.044 };
	lagLang.push(asd)

	var asd = { lat: -25.403, lng: 150.044 };
	lagLang.push(asd)

	var asd = { lat: -25.403, lng: 131.044 };
	lagLang.push(asd)

	for (var i=0; i<=lagLang.length; i++) {
		placeMarker(lagLang[i], "Marker Number " + i + 1)
	}

	
</script>

<script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD4RRQ0Y1lMgihqDFu1wVw1wPsb4bVtCmA&callback=initMap">
	
	<div id='map'></div>
		
